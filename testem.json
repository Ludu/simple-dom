{
  "test_page": "test/index.html",
  "parallel": 5,
  "before_tests": "npm run build",
  "src_files": [
    "lib/**/*.js",
    "test/*.js"
  ],
  "launchers": {
    "Node": {
      "command": "node test/node-runner.js testem",
      "protocol": "tap"
    }
  },
  "launch_in_ci":  ["Node", "PhantomJS"],
  "launch_in_dev":  ["Node", "Chrome", "Firefox", "Safari"]
}
